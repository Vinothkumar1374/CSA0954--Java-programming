import java.util.Scanner;
public class test2{
   public static void main(String[] args)
{
	Scanner input=new Scanner (System.in);
	System.out.print("enter the number: ");
	int num=input.nextInt();
        System.out.print("enter the n position value: ");
	int n=input.nextInt();

        int a[]=new int[100];
 	int x=0;
   
	for(int i=1;i<=num;i++)
	{
		if(num%i==0)
		{
		  a[x]=i;
		  x++;
		}
	}
    
	System.out.print("number of factors: " + x);
	System.out.println("\n" + n + "th factor is:" + a[n-1]);

}
}
